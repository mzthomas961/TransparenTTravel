<h1> Create your Agent Login </h1>

<%= form_for @agent do |f| %>
    <%=f.label :name %>
    <%=f.text_field :name%>
    <%= f.label :username %>
    <%=f.text_field :username %>
    <%=f.label :password %>
    <%=f.password_field :password %>
    <%= f.submit %>
    <% end %>

    <%= button_to "Back", login_path, method: "get"%>